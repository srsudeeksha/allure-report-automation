    "@vitest/ui": "^2.0.0",
    "allure-commandline": "^2.25.0",
    "allure-js-commons": "^3.4.1",
    "allure-vitest": "^3.4.1",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
